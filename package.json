{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web"
    ]
  },
  "scripts": {
    "compose:console": "docker compose -f ./docker-compose.dev.yml run --rm console /bin/bash",
    "compose:console:prod": "docker compose -f ./docker-compose.prod.yml -f ./docker-compose.override.yml run --rm console /bin/bash",
    "compose:build:dev": "docker compose -f ./docker-compose.dev.yml build",
    "compose:build:prod": "docker compose -f ./docker-compose.prod.yml build",
    "compose:run:dev": "docker compose -f ./docker-compose.dev.yml up",
    "compose:stop:dev": "docker compose -f ./docker-compose.dev.yml down",
    "compose:start:prod": "docker compose -f ./docker-compose.prod.yml  up",
    "compose:stop:prod": "docker compose -f ./docker-compose.prod.yml -f ./docker-compose.override.yml down -v",
    "compose:ps:prod": "docker compose -f ./docker-compose.prod.yml ps"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "devDependencies": {
    "@redwoodjs/core": "6.0.2",
    "@redwoodjs/web-server": "0.0.1"
  },
  "packageManager": "yarn@3.6.1",
  "engines": {
    "node": "=18.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  }
}
